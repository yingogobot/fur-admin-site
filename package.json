{
  "name": "vue-element-admin",
  "version": "4.2.1",
  "description": "FurFurLand Storage Admin System",
  "author": "YinXu <yin@furfurland.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "staging": "vue-cli-service serve --mode staging",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "new": "plop",
    "analyze": "cross-env NODE_ENV=production npm_config_report=true npm run build:prod",
    "wstart": "webpack-dev-server --hot --inline --config webpack/webpack-dev.js --api_env botme",
    "wstaging": "webpack-dev-server --hot --inline --config webpack/webpack-staging.js --api_env botme",
    "wbuild": "webpack --config webpack/webpack-prod.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "admin",
    "dashboard",
    "element-ui",
    "boilerplate",
    "admin-template",
    "management-system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PanJiaChen/vue-element-admin.git"
  },
  "bugs": {
    "url": "https://github.com/PanJiaChen/vue-element-admin/issues"
  },
  "dependencies": {
    "axios": "0.21.1",
    "clean-webpack-plugin": "^0.1.19",
    "codemirror": "5.45.0",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^1.0.0",
    "dropzone": "5.5.1",
    "echarts": "4.2.1",
    "element-ui": "2.7.0",
    "file-saver": "2.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fuse.js": "3.4.4",
    "happypack": "^5.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "inquirer": "^6.2.1",
    "js-cookie": "2.2.0",
    "jsonlint": "1.6.3",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "path-to-regexp": "2.4.0",
    "postcss-loader": "^2.1.6",
    "screenfull": "4.2.0",
    "shelljs": "^0.8.3",
    "showdown": "1.9.0",
    "sortablejs": "1.8.4",
    "style-loader": "^0.21.0",
    "tui-editor": "1.3.3",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.1.2",
    "verte": "0.0.12",
    "vue": "2.6.10",
    "vue-count-to": "1.0.13",
    "vue-moment": "^4.0.0",
    "vue-resource": "^1.5.1",
    "vue-router": "3.0.2",
    "vue-splitpane": "1.0.4",
    "vuedraggable": "2.20.0",
    "vuex": "3.1.0",
    "xlsx": "0.14.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/register": "7.0.0",
    "@vue/cli-plugin-babel": "3.5.3",
    "@vue/cli-plugin-eslint": "^3.9.1",
    "@vue/cli-plugin-unit-jest": "3.5.3",
    "@vue/cli-service": "3.5.3",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.5.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "chalk": "2.4.2",
    "chokidar": "2.1.5",
    "connect": "3.6.6",
    "cross-env": "^6.0.3",
    "eslint": "5.15.3",
    "eslint-plugin-vue": "5.2.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "1.3.1",
    "image-webpack-loader": "^4.6.0",
    "lint-staged": "8.1.5",
    "mockjs": "1.0.1-beta3",
    "node-sass": "^4.9.0",
    "plop": "2.3.0",
    "runjs": "^4.3.2",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "script-loader": "0.7.2",
    "serve-static": "^1.13.2",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.0",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.16.3",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.3.0",
    "webpack-merge": "^4.1.4",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
